{
  "name": "foo",
  "displayName": "foo",
  "publisher": "joelsaleem",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:foo.helloWorld",
    "onCommand:foo.refresh",
    "onView:foo-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "foo-sidebar-view",
          "title": "foo",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "foo-sidebar-view": [
        {
          "type": "webview",
          "id": "foo-sidebar",
          "name": "foo",
          "icon": "media/logo.svg",
          "contextualTitle": "foo"
        }
      ]
    },
    "commands": [
      {
        "command": "foo.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "foo.refresh",
        "category": "foo",
        "title": "Refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "@types/glob": "^7.1.3",
    "@types/jquery": "^3.5.6",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.42.3",
    "svelte-check": "^2.2.5",
    "svelte-preprocess": "^4.8.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@apollo/client": "^3.4.9",
    "graphql": "^15.5.1",
    "svelte-apollo": "^0.4.0"
  }
}
